            <a class="github" target="_blank" rel="noopener noreferrer" href=@Html.AttributeEncode("https://github.com/Benj1x")>
                <img src="/Content/Images/githubLogo.png" alt="Github logo" width="30px" />
            </a>
            <a class="youtube" target="_blank" rel="noopener noreferrer" href=@Html.AttributeEncode("https://www.youtube.com/channel/UCMCDZaVcotIl03Tt2l8Vs3w")>
                <img src="/Content/Images/yotubeLogo.png" alt="Youtube logo" width="35px" />
            </a>
            <h6 class="email">
                boh@studiobeans.dk
            </h6>
        </div>
    </div>
<div class="container-box-right">
    <h3>Benjamin O. Høj</h3>
    <img src="~/images/placeholder.jpg" alt="Billede af: Benjamin" asp-append-version="true" style="width:150px;height:150px; border-radius: 50%" />
